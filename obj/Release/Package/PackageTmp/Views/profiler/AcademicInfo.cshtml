    <script src="~/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <style>
        span.col_1.tleft.text-danger.field-validation-error :before {
            content: " Warning ! ";
            color: red;
            font-weight: bold;
        }

        .text-danger {
            color: red;
        }
    </style>
  
   
<script>
(function($){
  $(window).load(function(){

    $("#content-1").mCustomScrollbar({
      axis:"yx", //set both axis scrollbars
      advanced:{autoExpandHorizontalScroll:true}, //auto-expand content to accommodate floated elements
      // change mouse-wheel axis on-the-fly 
      callbacks:{
        onOverflowY:function(){
          var opt=$(this).data("mCS").opt;
          if(opt.mouseWheel.axis!=="y") opt.mouseWheel.axis="y";
        },
        onOverflowX:function(){
          var opt=$(this).data("mCS").opt;
          if(opt.mouseWheel.axis!=="x") opt.mouseWheel.axis="x";
        },
      }
    });

  });
})(jQuery);
</script>
</head>
<body id="ad">
    <div class="container">
        <div class="row sub-container">
            <div class="col-md-6 col-sm-12 col-xs-12">
                <div class="top-logos">
                    <img src="~/images/qendidate-logo.png" />
                    <img src="~/images/orion-logo.png" />
                </div>
            </div>

            @{ var qenid = ViewBag.qenid;}
            <div class="col-md-6 col-sm-12 col-xs-12">
                <div class="right-box">
                    <div class="form" id="content-1">    
                        
                            <h1>academic information</h1>
                        <div id="accordion" role="tablist" aria-multiselectable="true">
                            <div class="card">
                                <div class="card-header" role="tab" id="headingOne">
                                    <h5 class="mb-0">
                                        <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                            High School
                                        </a>
                                    </h5>
                                </div>

                                <div id="collapseOne" class="collapse" role="tabpanel" aria-labelledby="headingOne">
                                    <div class="card-block">
                                        @Html.Action("highSchool", "profiler", new { qenid = @qenid })
                                    </div>
                                </div>
                            </div>
                            <div class="card">
                                <div class="card-header" role="tab" id="headingTwo">
                                    <h5 class="mb-0">
                                        <a class="collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                            Higher School
                                        </a>
                                    </h5>
                                </div>
                                <div id="collapseTwo" class="collapse" role="tabpanel" aria-labelledby="headingTwo">
                                    <div class="card-block">
                                        @Html.Action("higherSchool", "profiler", new { qenid = @qenid })
                                    </div>
                                </div>
                            </div>
                            <div class="card">
                                <div class="card-header" role="tab" id="headingThree">
                                    <h5 class="mb-0">
                                        <a class="collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                                            Graduation
                                        </a>
                                    </h5>
                                </div>
                                <div id="collapseThree" class="collapse" role="tabpanel" aria-labelledby="headingThree">
                                    <div class="card-block">
                                        @Html.Action("Graduation", "profiler", new { qenid = @qenid })
                                    </div>
                                </div>
                            </div>
                            <div class="card">
                                <div class="card-header" role="tab" id="headingfour">
                                    <h5 class="mb-0">
                                        <a class="collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapsefour" aria-expanded="false" aria-controls="collapsefour">
                                            Post Graduation
                                        </a>
                                    </h5>
                                </div>
                                <div id="collapsefour" class="collapse" role="tabpanel" aria-labelledby="headingfour">
                                    <div class="card-block">
                                        @Html.Action("PostGraduation", "profiler", new { qenid = @qenid })
                                    </div>
                                </div>
                            </div>
                        </div>


                      
                    </div>


               
                    <div class="group next-btn-wrap">
                        <a class="prev-btn" onclick="location.href='@Url.Action("PersonalInfo", "profiler",new { qenid = @qenid })'">Previous</a>
                        <a class="nxt-btn" onclick="location.href='@Url.Action("certifications", "profiler",new { qenid = @qenid })'">Next</a>
                    </div>

                </div>
            </div>
            <div class="left-text">
                <p>Achieve your dream by jumpstarting with Qendidate</p>
            </div>
        </div>

    </div>


