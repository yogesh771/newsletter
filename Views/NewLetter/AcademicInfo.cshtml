@using System.Web.Mvc.Html
@model NewLetter.Models.AcademicModel
@{
    ViewBag.Title = "AcademicInfo";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>Academic Information</title>
    <link href="~/css/style.css" rel="stylesheet">
    <!-- Bootstrap -->
    <link href="~/css/bootstrap.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans|Roboto" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Oswald" rel="stylesheet">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <style>
        span.col_1.tleft.text-danger.field-validation-error :before {
            content: " Warning ! ";
            color: red;
            font-weight: bold;
        }

        .text-danger {
            color: red;
        }
    </style>
</head>
<body id="ad">
    <div class="container">
        <div class="row sub-container">
            <div class="col-md-6 col-sm-12 col-xs-12">
                <div class="top-logos">
                    <img src="~/images/qendidate-logo.png" />
                    <img src="~/images/orion-logo.png" />
                </div>
            </div>

            @{ var qenid = ViewBag.qenid;}

            <div class="col-md-6 col-sm-12 col-xs-12">

                <div class="right-box">



                    <div class="form">
                        <form>
                            <h1>academic information</h1>

                            <div id="education">  </div>
                            <table class="table">
                                <thead>
                                    <tr>
                                       
                                        <th>Education</th>
                                        <th>

                                        </th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                       
                                        <td>High School</td>
                                        <td>
                                            <div class="group plus-btn-wrap m-top">
                                                <a class="plus-btn"><img src="/images/plus-btn.png" onclick="return highSchool();" /></a>
                                            </div>
                                        </td>

                                    </tr>
                                    <tr>
                                       
                                        <td>Higher School</td>
                                        <td>
                                            <div class="group plus-btn-wrap m-top">
                                                <a class="plus-btn"><img src="/images/plus-btn.png" onclick="return higherSchool();" /></a>
                                            </div>
                                        </td>

                                    </tr>
                                    <tr>
                                        
                                        <td>Graduation</td>
                                        <td>
                                            <div class="group plus-btn-wrap m-top">
                                                <a class="plus-btn"><img src="/images/plus-btn.png" onclick="return Graduation();" /></a>
                                            </div>
                                        </td>

                                    </tr>
                                    <tr>
                                      
                                        <td>Post Graduation</td>
                                        <td>
                                            <div class="group plus-btn-wrap m-top">
                                                <a class="plus-btn"><img src="/images/plus-btn.png" onclick="return PostGraduation();" /></a>
                                            </div>
                                        </td>

                                    </tr>
                                </tbody>
                            </table>

                        </form>

                        <div class="group next-btn-wrap">
                            <a class="prev-btn" onclick="location.href='@Url.Action("PersonalInfo", "NewLetter",new { qenid = @qenid })'" >Previous</a>
                            <a class="nxt-btn" onclick="location.href='@Url.Action("certifications", "NewLetter",new { qenid = @qenid })'">Next</a>
                        </div>



                    </div>

                </div>
            </div>
            <div class="left-text">
                <p>Achieve your dream by jumpstarting with Qendidate</p>
            </div>
        </div>

    </div>


    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js" defer></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/js/bootstrap.min.js" defer></script>

   

    <script>
        function highSchool()
        {
            debugger
            var URL = '@Url.Action("highSchool", "NewLetter")?qenid='+'@qenid';
            $.get(URL, function (data) {
                debugger
                $("#education").html(data);
            });
        }
    </script>

    <script>
        function higherSchool() {
            debugger
            var URL = '@Url.Action("higherSchool", "NewLetter")?qenid=' + '@qenid';
            $.get(URL, function (data) {
                debugger
                $("#education").html(data);
            });
        }
    </script>
    <script>
        function Graduation()
        {
            debugger
            var URL = '@Url.Action("Graduation", "NewLetter")?qenid='+'@qenid';
            $.get(URL, function (data) {
                debugger
                $("#education").html(data);
            });
        }
    </script>
    <script>
        function PostGraduation()
        {
            debugger
            var URL = '@Url.Action("PostGraduation", "NewLetter")?qenid='+'@qenid';
            $.get(URL, function (data) {
                debugger
                $("#education").html(data);
            });
        }
    </script>
</body>
</html>  